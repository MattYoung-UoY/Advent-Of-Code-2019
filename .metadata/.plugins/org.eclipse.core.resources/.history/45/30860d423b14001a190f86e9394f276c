package main;

import java.io.BufferedReader;
import java.io.File;
import java.io.InputStreamReader;

public class Main {

	public static void main(String[] args) {
		
	}
	
	public int[] inputs() {
		File in = new File("input.txt");
		BufferedReader br = new BufferedReader(new InputStreamReader(Class.class.getResourceAsStream("input.txt")));
	}
	
	public static int moduleFuel(int mass) {
		float res = mass;
		
		res /= 3;
		res -= (res %= 1);
		res -= 2;
		
		return (int) res;
	}
	
}
