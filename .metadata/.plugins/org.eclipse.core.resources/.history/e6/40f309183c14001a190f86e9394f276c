package main;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileInputStream;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.ArrayList;
import java.util.List;

public class Main {

	public static void main(String[] args) {
		System.out.println(sum(inputs()));
	}
	
	public static int[] inputs() {
		List<Integer> masses = new ArrayList<Integer>();
		try {
			BufferedReader br = new BufferedReader(new InputStreamReader(new FileInputStream(new File("src/main/input.txt"))));
			String line;
			while((line = br.readLine()) != null) masses.add(Integer.parseInt(line));
			br.close();
		} catch (IOException e) {
			e.printStackTrace();
		}
		int[] res = new int[masses.size()];
		for(int i = 0; i < res.length; i++) res[i] = masses.get(i);
		return res;
	}
	
	public static int moduleFuel(int mass) {
		float res = mass;
		
		res /= 3;
		res -= (res %= 1);
		res -= 2;
		
		return (int) res;
	}
	
}
